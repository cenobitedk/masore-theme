{% extends "base.twig" %}

{% set body_class = [body_class, post.title|lower]|join(' ') %}

{% block main %}
  <div class="content-wrapper">
    {{post.content}}
    {# <div class="img-wrap">
      <img src="{{TimberImage(post.thumbnail).src('large')}}" class="frontpage__image" title="{{ post.thumbnail.caption }}" alt="Image for {{post.title}}"/>
    </div> #}
    <div class="media-wrap">
      <video
        id="video"
        loop
        autoplay
        muted
        preload
        poster="/media/snapshot.jpg">
        <source src="/media/SS20.webm" type="video/webm">
        <source src="/media/SS20.mp4" type="video/mp4">
      </video>
      <div class="media-icon mute">
        <img alt="mute icon" src="data:image/svg+xml;base64,">
      </div>
      <div class="media-icon audio">
        <img alt="audio icon" src="data:image/svg+xml;base64,">
      </div>
    </div>
  </div>
{% endblock %}
